        </actions>
        <actions>
            <name>Status_Pending_DocuSign</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <formula>AND(    OR(      Approval_Status_DocuSign__c = &quot;Sent&quot; ,      Approval_Status_DocuSign__c = &quot;Viewed&quot; ,     Approval_Status_DocuSign__c = &quot;Voided&quot; ,     Approval_Status_DocuSign__c = &quot;Declined&quot; ,     AND(       Approval_Status_DocuSign__c = &quot;&quot;,       Pre_Approved__c = FALSE     )   ),     ISPICKVAL( Approval_Status_Salesforce__c , &quot;Approved&quot; ) )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Contract - Pending Salesforce Status - Update</fullName>
        <actions>
            <name>Status_Pending_Salesforce</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <formula>AND(   OR(     Approval_Status_DocuSign__c = &quot;Completed&quot; ,     AND(       Approval_Status_DocuSign__c = &quot;&quot;,       Pre_Approved__c = TRUE     )   ),   OR(     ISPICKVAL( Approval_Status_Salesforce__c , &quot;Pending NCM&quot; ),     ISPICKVAL( Approval_Status_Salesforce__c , &quot;Pending Internal&quot; ),     ISPICKVAL( Approval_Status_Salesforce__c , &quot;Recalled&quot; ),     ISPICKVAL( Approval_Status_Salesforce__c , &quot;Rejected&quot; ),     ISPICKVAL( Approval_Status_Salesforce__c , &quot;&quot; )   ) )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Contract - Resend to Customer Status - Email %2F Update</fullName>
        <actions>
            <name>Home_Services_Contract_eSignature_Rejected</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>Status_Resend_to_Customer</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <formula>OR(
  DocuSign_eSignature_Status__c = &quot;Voided&quot;,
  DocuSign_eSignature_Status__c = &quot;Declined&quot;
)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Delivery Date Populated</fullName>
        <actions>
            <name>Status_to_Project_Created</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Contract.Delivery_Date__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>New Contract Created</fullName>
        <actions>
            <name>New_Contract_Created</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Contract.CreatedDate</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <triggerType>onCreateOnly</triggerType>
    </rules>
</Workflow>
