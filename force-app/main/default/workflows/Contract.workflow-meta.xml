    <rules>
        <fullName>Approved Credit</fullName>
        <actions>
            <name>Credit_Approved</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Contract.Credit_Status__c</field>
            <operation>equals</operation>
            <value>Approved</value>
        </criteriaItems>
        <criteriaItems>
            <field>Contract.Status</field>
            <operation>equals</operation>
            <value>Pending Credit Approval</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Contract - Activated Status - Email %2F Update</fullName>
        <actions>
            <name>Home_Services_Contract_eSignature_Completed</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>Status_Activated</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <formula>DocuSign_eSignature_Status__c = &quot;Completed&quot;</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Contract - Activated by Admin %28Activate Button%29 - Update</fullName>
        <actions>
            <name>Status_Activated</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <description>Changes the Status of a Contract to Activated when the Activated button is clicked by a System Administrator.</description>
        <formula>AND(
  NOT( ISPICKVAL( Status , &quot;Activated&quot;) ) ,
  ISCHANGED( ActivatedDate )
)

&amp;&amp; $User.ProfileId = &quot;00e30000001FeAR&quot; /* System Administrator */</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Contract - Draft Status - Update</fullName>
        <actions>
            <name>Status_Draft</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <formula>OR(   Approval_Status_DocuSign__c = &quot;Voided&quot; ,   Approval_Status_DocuSign__c = &quot;Declined&quot; ,   AND(     Approval_Status_DocuSign__c = &quot;&quot;,     Pre_Approved__c = FALSE   ),   ISPICKVAL( Approval_Status_Salesforce__c , &quot;Recalled&quot; ),   ISPICKVAL( Approval_Status_Salesforce__c , &quot;Rejected&quot; ),   ISPICKVAL( Approval_Status_Salesforce__c , &quot;&quot; ) )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Contract - In Approval Process Status - Update</fullName>
        <actions>
            <name>Status_In_Approval_Process</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <formula>AND(    OR(      Approval_Status_DocuSign__c = &quot;Sent&quot; ,      Approval_Status_DocuSign__c = &quot;Viewed&quot;,      Approval_Status_DocuSign__c = &quot;&quot;    ),    OR(      ISPICKVAL( Approval_Status_Salesforce__c , &quot;Pending NCM&quot; ),     ISPICKVAL( Approval_Status_Salesforce__c , &quot;Pending Internal&quot; )   )  )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Contract - Notify AM of Referral Fee or Rebate - Email</fullName>
        <actions>
            <name>Referral_Fee_Rebate_Notification</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Contract.Referral_Fee_or_Rebate__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Contract.Referral_Fee_Rebate_Language__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Contract.Status</field>
            <operation>equals</operation>
            <value>Activated</value>
        </criteriaItems>
        <criteriaItems>
            <field>Contract.ActivatedDate</field>
            <operation>equals</operation>
            <value>TODAY</value>
        </criteriaItems>
        <description>If a Referral Fee or Rebate is included in the contract, once activated an email will trigger to Analytics and Finance (AM) to proceed with reporting and payment.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Contract - Out for eSignature Status - Email %2F Update</fullName>
        <actions>
