    <rules>
        <fullName>POD Requested Follow Up</fullName>
        <active>true</active>
        <criteriaItems>
            <field>Case.POD_Requested_Date__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Status</field>
            <operation>equals</operation>
            <value>POD Requested</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>POD_Follow_Up_Task</name>
                <type>Task</type>
            </actions>
            <offsetFromField>Case.POD_Requested_Date__c</offsetFromField>
            <timeLength>20</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>Status_to_Back_Charge</name>
                <type>FieldUpdate</type>
            </actions>
            <offsetFromField>Case.POD_Requested_Date__c</offsetFromField>
            <timeLength>24</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>Finial_Follow_up</name>
                <type>Task</type>
            </actions>
            <offsetFromField>Case.POD_Requested_Date__c</offsetFromField>
            <timeLength>22</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Pending Customer Process</fullName>
        <active>true</active>
        <criteriaItems>
            <field>Case.Status</field>
            <operation>equals</operation>
            <value>Pending Customer Response</value>
        </criteriaItems>
        <description>Assigns task to case owner and then closes the case w/ no response.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Case_Closed</name>
                <type>FieldUpdate</type>
            </actions>
            <timeLength>8</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>Final_Call_Attempt</name>
                <type>Task</type>
            </actions>
            <timeLength>6</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>X2nd_Call_Attempt</name>
                <type>Task</type>
            </actions>
            <timeLength>4</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>X1st_Call_Attempt</name>
                <type>Task</type>
            </actions>
            <timeLength>2</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Post%2FPre Delivery Task</fullName>
        <actions>
            <name>Post_delivery_call</name>
            <type>Task</type>
        </actions>
        <actions>
            <name>Pre_Delivery_Call1</name>
            <type>Task</type>
        </actions>
        <actions>
            <name>X24_Hour_Delivery_Check</name>
            <type>Task</type>
        </actions>
        <active>true</active>
        <description>Creates task for deliveries</description>
        <formula>OR(
RecordType.Name = &quot;Order Intake&quot;,
RecordType.Name = &quot;Post-Delivery OM&quot;,
RecordType.Name = &quot;Pre-Delivery OM&quot;)
&amp;&amp;
ISCHANGED(Estimated_Delivery_Date__c)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
