    <rules>
        <fullName>Case - CSS Assignment Customer Auto Email - External Email</fullName>
        <actions>
            <name>CSS_Assignment_Auto_Email_Alert</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>Send an email to the Contact when an order is passed from the field to the CSS</description>
        <formula>AND( NOT(ISBLANK(ContactId)), NOT(ISBLANK(Contact.Email)), ISCHANGED(OwnerId), NOT(BEGINS(PRIORVALUE(OwnerId), &quot;005&quot;)), CONTAINS(RecordType.DeveloperName, &quot;Customer_Success&quot;) )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Case - Closed and Escalated to Product Managers - Email</fullName>
        <actions>
            <name>Case_Closed_Escalated_to_Product_Managers</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Case.IsClosed</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Type</field>
            <operation>equals</operation>
            <value>CSI Team,Inventory Replenishment,Product Manager Support</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Escalate_to_Product_Manager__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <description>Case was closed by orginal team and escalated to Product Managers for further responce.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
