<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Contract_Complete</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISCHANGED( Status ) , TEXT(PRIORVALUE( Status ))==&quot;Complete&quot; )</errorConditionFormula>
    <errorMessage>The status of contract once completed, cannot be changed</errorMessage>
</ValidationRule>
