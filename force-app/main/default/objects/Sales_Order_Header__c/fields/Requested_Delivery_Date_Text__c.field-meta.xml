<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Requested_Delivery_Date_Text__c</fullName>
    <description>Convert the delivery date to text for displaying on Account page</description>
    <externalId>false</externalId>
    <formula>TEXT(MONTH(Requested_Delivery_Date__c))+&quot;/&quot; +TEXT(DAY(Requested_Delivery_Date__c))+&quot;/&quot; +TEXT(YEAR(Requested_Delivery_Date__c))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Requested Delivery Date Text</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
