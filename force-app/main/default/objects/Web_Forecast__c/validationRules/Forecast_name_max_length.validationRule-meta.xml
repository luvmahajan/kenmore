<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Forecast_name_max_length</fullName>
    <active>true</active>
    <description>max length of the forecast name is 35 character</description>
    <errorConditionFormula>LEN( Name )  &gt;  35</errorConditionFormula>
    <errorDisplayField>Name</errorDisplayField>
    <errorMessage>Limit to 35 characters</errorMessage>
</ValidationRule>
