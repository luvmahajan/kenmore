<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AdditionalDiscountAmount__c</fullName>
    <externalId>false</externalId>
    <formula>IF( OR(ISNULL(TEXT(Discount_Operator__c)),ISNULL(Additional_Disc__c),ISNULL(List_Unit_Price__c)), 0, IF(OR(TEXT(Discount_Operator__c)==&quot;Amount&quot;,TEXT(Discount_Operator__c)==&quot;&quot;,TEXT(Discount_Operator__c)==null), Additional_Disc__c, (List_Unit_Price__c*Additional_Disc__c)/100))</formula>
    <label>Additional Disc. (Amt)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
