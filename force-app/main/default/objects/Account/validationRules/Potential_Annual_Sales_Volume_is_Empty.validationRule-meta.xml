<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Potential_Annual_Sales_Volume_is_Empty</fullName>
    <active>false</active>
    <errorConditionFormula>AND(IF(ISCHANGED(Automation_Control__c),
   IF( CONTAINS(Automation_Control_Type__c, &quot;Validation&quot;),FALSE,TRUE),TRUE),
ISPICKVAL(Type, &quot;Customer&quot;),
SAP_Account_Request__c,
ISPICKVAL(Potential_Annual_Sales_Volume__c, &quot;&quot;))</errorConditionFormula>
    <errorDisplayField>Potential_Annual_Sales_Volume__c</errorDisplayField>
    <errorMessage>Potential Annual Sales Volume is required.</errorMessage>
</ValidationRule>
