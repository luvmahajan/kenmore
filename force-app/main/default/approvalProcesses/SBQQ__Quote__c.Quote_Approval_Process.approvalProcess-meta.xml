<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>true</allowRecall>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>Owner</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>michelle.wiggins@kenmoredirect.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>SBQQ__Quote__c.RecordType</field>
                <operation>equals</operation>
                <value>Commercial - Contract</value>
            </criteriaItems>
            <criteriaItems>
                <field>SBQQ__Quote__c.SBQQ__Status__c</field>
                <operation>equals</operation>
                <value>Approval</value>
            </criteriaItems>
            <criteriaItems>
                <field>SBQQ__Quote__c.SBQQ__SalesRep__c</field>
                <operation>equals</operation>
                <value>0053A00000CatLHQAZ</value>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Project Team to Approve 1</label>
        <name>Project_Team_to_Approve_1</name>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>michelle.wiggins@kenmoredirect.com</name>
                <type>user</type>
            </approver>
            <approver>
                <name>sharon.younce@kenmoredirect.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>SBQQ__Quote__c.RecordType</field>
                <operation>equals</operation>
                <value>Commercial - Contract,Commercial - Contract</value>
            </criteriaItems>
            <criteriaItems>
                <field>SBQQ__Quote__c.SBQQ__Status__c</field>
                <operation>equals</operation>
                <value>Approval</value>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Project Team to Approve 2</label>
        <name>Project_Team_to_Approve_2</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <name>jscott.wertman@kenmoredirect.com</name>
                <type>user</type>
            </approver>
            <approver>
                <name>ken.crowne@kenmoredirect.com</name>
                <type>user</type>
            </approver>
            <approver>
                <name>robert.lederer@kenmoredirect.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <formula>RecordType.DeveloperName == &#39;Commercial_Flow_Business&#39;</formula>
        </entryCriteria>
        <label>Margin Approval</label>
        <name>Margin_Approval</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <description>10/7/2019 Approval Process</description>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <finalApprovalActions>
        <action>
            <name>Document_Prep_Stage</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Quote_Result_Alert</name>
            <type>Alert</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Add_Product_Status</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Quote_Result_Alert</name>
            <type>Alert</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <label>Quote Approval Process</label>
    <recallActions>
        <action>
            <name>Add_Product_Status</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Quote_Result_Alert</name>
            <type>Alert</type>
        </action>
    </recallActions>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
