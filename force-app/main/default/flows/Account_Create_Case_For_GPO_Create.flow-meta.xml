    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
IF(ISCHANGED([Account].Automation_Control__c ),  IF(CONTAINS([Account].Automation_Control_Type__c, &quot;Process&quot;), FALSE,TRUE),TRUE),
NOT(ISPICKVAL([Account].GPO__c, &quot;&quot;)),
CONTAINS(PRIORVALUE([Account].SAP_Account_ID__c), &quot;Processing&quot;) ,
BEGINS( [Account].SAP_Account_ID__c, &quot;001&quot;))</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
